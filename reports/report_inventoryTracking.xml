<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <record id="paperformat_request_landscape" model="report.paperformat">
        <field name="name">Requests Landscape</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Landscape</field>
        <field name="dpi">90</field>
        </record>


        <record id="inventory_track_inventory_rpts" model="ir.actions.report">
            <field name="name">Asset Transfer Report</field>
            <field name="model">inventory_track.inventory</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">InventoryTracking.inventory_rpt</field>
            <field name="report_file">InventoryTracking.inventory_rpt</field>
            <field name="print_report_name">'Asset Transfer  - %s' % (object.tag.tag or '')  </field>
            <field name="binding_model_id" ref="model_inventory_track_inventory"/>
            <field name="binding_type">report</field>
        </record>

         <record id="inventory_track_inventory_rpts_diagnosis" model="ir.actions.report">
            <field name="name">Asset Transfer Diagnosis Report</field>
            <field name="model">inventory_track.inventory</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">InventoryTracking.inventory_rpts_diagnosis</field>
            <field name="report_file">InventoryTracking.inventory_rpts_diagnosis</field>
            <field name="print_report_name">'Asset Transfer  - %s' % (object.tag.tag or '')  </field>
            <field name="binding_model_id" ref="model_inventory_track_inventory"/>
            <field name="binding_type">report</field>
        </record>

        <record id="inventory_track_inventory_checklist_rpts" model="ir.actions.report">
            <field name="name">Asset Checklist Report</field>
            <field name="model">inventory_track.inventory</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">InventoryTracking.inventory_checklist_rpts</field>
            <field name="report_file">InventoryTracking.inventory_checklist_rpts</field>
            <field name="print_report_name">'Asset Checklist  - %s' % (object.tag.tag or '')  </field>
            <field name="binding_model_id" ref="model_inventory_track_inventory"/>
            <field name="binding_type">report</field>
        </record>
    

    <report
        id="inventory_report_xlsx"
        model="inventory_track.inventory"
        string="Print Inventory to XLSX"
        report_type="xlsx"
        name="inventory_track.inventory_list"
        file="Inventory"
        attachment_use="False"/>


    </data>
</odoo>



